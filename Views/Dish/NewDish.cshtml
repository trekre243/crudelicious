@model Dish
<form asp-action="Create" asp-controller="Dish" method="post">
    <div class="row">
        <div class="col s12">
            <h3>Add a New Dish</h3>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            <a href="/">Home</a>
        </div>
    </div>
    <div class="row">
        <div class="col s4">
            <div class="input-field col s12">
                <input asp-for="Chef" type="text">
                <label asp-for="Chef"></label>
                <span asp-validation-for="Chef"></span>
            </div>
            <div class="input-field col s12">
                <input asp-for="Name" type="text">
                <label asp-for="Name"></label>
                <span asp-validation-for="Name"></span>
            </div>
            <div class="input-field col s12">
                <input asp-for="Calories" type="text">
                <label asp-for="Calories"></label>
                <span asp-validation-for="Calories"></span>
            </div>
        </div>
        <div class="col s4">
            <label asp-for="Tastiness"></label>
            <select class="browser-default" asp-for="Tastiness">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
            </select>
            <span asp-validation-for="Tastiness"></span>
        </div>
        <div class="col s4">
            <textarea asp-for="Description" class="materialize-textarea"></textarea>
            <label asp-for="Description"></label>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            <button class="waves-effect waves-light btn" type="submit">Add Dish</button>
        </div>
    </div>
</form>